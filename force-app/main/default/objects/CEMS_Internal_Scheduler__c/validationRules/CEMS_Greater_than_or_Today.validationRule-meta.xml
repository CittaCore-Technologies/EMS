<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CEMS_Greater_than_or_Today</fullName>
    <active>true</active>
    <errorConditionFormula>CEMS_Interview_Start_Time__c &lt;= DATETIMEVALUE( CreatedDate )</errorConditionFormula>
    <errorDisplayField>CEMS_Interview_Start_Time__c</errorDisplayField>
    <errorMessage>To proceed with the interview start time, please select a date that is equal to or later than today&apos;s date</errorMessage>
</ValidationRule>
